<?xml version="1.0" encoding="UTF-8"?>
<custom xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../xsd/custom.xsd">

	<view id="hierarchy_tester_sel" view.parameter="DISTINCT">
		<viewObject id="VO_hierarchy_tester_sel_1" objectType="USERGROUP"
			isMainViewObject="true">
			<viewColumn id="client_sign" attributeType="client_sign" />
			<viewColumn id="tester_obj_id" attributeType="obj_id"
				isSortable="true" isSelectColumn="true" />
			<viewColumn id="name" attributeType="name" isSortable="true"
				isSelectColumn="true" />
			<viewColumn id="description" attributeType="description"
				isSortable="true" isSelectColumn="true" />
			<viewColumn id="tester_version_active" attributeType="version_active" />
			<viewColumn id="deactivated" attributeType="deactivated"
				isSelectColumn="true" />
			<viewColumn id="tester_role" attributeType="role" />
		</viewObject>
		<conditions>
			<compoundCondition conditionConnect="AND">
				<condition column.id="name" compareType="LIKE" />
				<condition column.id="description" compareType="LIKE" />
				<condition column.id="tester_obj_id" compareType="NOTIN"
					refView.id="hierarchy_tester_sel_sub" />
				<condition column.id="client_sign" compareType="EQ"
					currentObjectType.id="HIERARCHY" currentAttributeType.id="client_sign" />
				<condition column.id="tester_version_active" compareType="EQ"
					value="true" />
				<condition column.id="deactivated" compareType="EQ" value="false" />
				<compoundCondition conditionConnect="OR">
					<condition column.id="tester_role" compareType="EQ" value="tester" />
					<condition column.id="tester_role" compareType="EQ" value="tester2l" />
					<condition column.id="tester_role" compareType="EQ" value="tester3l" />
				</compoundCondition>
			</compoundCondition>
		</conditions>
	</view>

</custom>