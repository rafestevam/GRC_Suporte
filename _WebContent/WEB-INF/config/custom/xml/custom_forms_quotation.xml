<?xml version="1.0" encoding="UTF-8"?>
<custom xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/custom.xsd">
    
	<form id="FORM_QUOTATION">
        <logToolbarButtons>
            <buttonReference button.idref="ARIS_REPORT"/>
        </logToolbarButtons>
        <formButtons>
            <buttonReference button.idref="OBJECT_SAVE"/>
            <buttonReference button.idref="OBJECT_SAVE_AND_NEXT"/>
            <buttonReference button.idref="PDF_REPORT"/>
        </formButtons>
        <page id="quotation">
            <rowGroup id="quotation.1">
                <row id="name">
                    <element attribute.idref="name" template="impactTypeName"/>
                </row>
                <row id="description">
                    <element attribute.idref="description"/>
                </row>
                
                <!-- Inicio GAP GRC29 -->
                <!-- 
                <row id="height">
                	<element attribute.idref="height"/>
                </row>
                 -->
                <!-- Fim GAP GRC29 -->
                
                <row id="value">
                    <element attribute.idref="value"/>
                </row>
            </rowGroup>
        </page>
    </form>     
    
</custom>
