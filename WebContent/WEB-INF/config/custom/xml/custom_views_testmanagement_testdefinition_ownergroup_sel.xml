<?xml version="1.0" encoding="UTF-8"?>
<custom xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../xsd/custom.xsd">

	<view id="testdefinition_ownergroup_sel" view.parameter="DISTINCT">
		<viewObject id="VO_testdefinition_ownergroup_sel_1"
			objectType="USERGROUP" isMainViewObject="true"
			skipRightsConditionForRoleLevels="object">
			<viewColumn id="client_sign" attributeType="client_sign"
				isGrouped="true" />
			<viewColumn id="ownergroup_obj_id" attributeType="obj_id"
				isGrouped="true" isSortable="true" isSelectColumn="true" />
			<viewColumn id="ownergroup_name" attributeType="name"
				isGrouped="true" isSortable="true" isSelectColumn="true" />
			<viewColumn id="ownergroup_description" attributeType="description"
				isGrouped="true" isSortable="true" isSelectColumn="true" />
			<viewColumn id="ownergroup_version_active" attributeType="version_active" />
			<viewColumn id="deactivated" attributeType="deactivated"
				isGrouped="true" isSortable="true" isSelectColumn="true" />
			<viewColumn id="ownergroup_role" attributeType="role"
				isSortable="true" />
		</viewObject>
		<viewObject id="VO_testdefinition_ownergroup_sel_2"
			objectType="HIERARCHY">
			<viewColumn id="hierarchy_version_active" attributeType="version_active"
				isGrouped="true" />
			<viewColumn id="hierarchy_count" attributeType="obj_id"
				isSelectColumn="true" function="COUNT_DISTINCT" />
		</viewObject>
		<link linktype.id="69" firstViewObject="VO_testdefinition_ownergroup_sel_1"
			secondViewObject="VO_testdefinition_ownergroup_sel_2" joinType="LEFT" />
		<conditions>
			<compoundCondition conditionConnect="AND">
				<condition column.id="ownergroup_name" compareType="LIKE" />
				<condition column.id="ownergroup_description" compareType="LIKE" />
				<condition column.id="ownergroup_obj_id" compareType="NOTIN"
					currentObjectType.id="TESTDEFINITION" currentAttributeType.id="owner_group" />
				<condition column.id="client_sign" compareType="EQ"
					currentObjectType.id="TESTDEFINITION" currentAttributeType.id="client_sign" />
				<condition column.id="ownergroup_version_active"
					compareType="EQ" value="true" />
				<condition column.id="deactivated" compareType="EQ" value="false" />
				<compoundCondition conditionConnect="OR">
					<condition column.id="ownergroup_role" compareType="EQ"
						value="tester" />
					<condition column.id="ownergroup_role" compareType="EQ"
						value="tester2l" />
				</compoundCondition>
				<compoundCondition conditionConnect="OR">
					<condition column.id="hierarchy_version_active"
						compareType="EQ" value="true" />
					<condition column.id="hierarchy_version_active"
						compareType="ISNULL" />
				</compoundCondition>
			</compoundCondition>
		</conditions>
	</view>

</custom>