<?xml version="1.0" encoding="UTF-8"?>
<custom xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../xsd/custom.xsd">

	<segregationsOfDuties>
	
		<segregationOfDuties objectType="testcase">
			<roleBased>
				<segregationsOfDuties.role id="tester2l"/>
				<segregationsOfDuties.role id="testreviewer"/>
			</roleBased>
			<userBased>
				<segregationsOfDuties.rule currentUserRole.idref="tester2l" notAllowedInAttributes="reviewer, reviewer_substitute" />
				<segregationsOfDuties.rule currentUserRole.idref="testreviewer" notAllowedInAttributes="owner, owner_substitute" />
			</userBased>
		</segregationOfDuties>
	
	</segregationsOfDuties>

</custom>